kernel = "rump-kernel"
memory = 16
# vcpus == 1 required
vcpus=1
name = "rump-kernel"
disk = [ 'file:test.ffs,hda,rw', 'file:etc.ffs,hdb,rw' ]
vif = [ 'mac=b2:11:11:11:11:11' ]

# if 1, run fs test
# if 2, run net test
# if 4, run httpd
# or any bitwise combination thereof
# note: both "2" and "4" run forever, so "6" is effectively "2"
extra = "4"

on_crash="preserve"
